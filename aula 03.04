#include <stdio.h>

int main(){
    /*Desenvolva um programa onde o usuario informara um valor (valido) entre 1 e 100
    
    exemplo: supondo que o usuario digitou o numero 5 retorne */

    int valor=0,i=0,j=0;

    do{
    	printf("Digite um valor de 1 a 100\n");
        scanf("%d",&valor);
	}while(valor<0 || valor>100);
	
	j=valor;
	for(i=1;i<=valor;i++,j--){
		printf("%d\t%d\n",i,j);
	}
	
	return 0;
}
